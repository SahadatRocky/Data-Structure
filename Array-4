#include <iostream>

using namespace std;

int main()
{
    int n;
    int count = 0;
    int x;
    cout << "Size of The Array : ";
    cin >> n;   // 10

    int *arr = new int[n];


    cout << "Enter elements in array: ";
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }

    cout << "What do you want to search: ";
    cin >> x;
    for (int i = 0; i < n; i++)
    {
        if (arr[i] == x)  // 6 == 6
        {
            count = count + 1;

        }

    }
    cout << "the number occurs " << count << " times in the array" << endl;

    return 0;
}
