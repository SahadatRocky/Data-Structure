#include<iostream>
using namespace std;

bubble_sort(int *arr , int n);
bubble_sort(int *arr , int n)
{
    for(int i=n-1;i>0;i++)
    {
        for(int j=0;j<i;j++)
        {
            if(arr[j]>arr[j+1])
            {
                int temp;
                temp=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
            }
        }
    }
}
int main()
{
    int n;
    cout<<"ener the number is:";
    cin>>n;
    int *arr=new int[n];

     for(int i=0;i<n;i++)
     {
         cin>>arr[i];
     }

    bubble_sort(arr, n);
   cout<<"bubble sorting is"
    for(int i=0;i<n;i++)
     {
         cout<<arr[i]<<endl;
     }
cout<<endl;



 return 0;
}
